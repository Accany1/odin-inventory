<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
</head>
<body>
  <h1><%= title %></h1>
    <% inventory.forEach(function(entry) { %>
    <div>
        <div>Trainer: <%= entry.name %></div>
        <div>Pokemon: <%= entry.pokename %>, Category: <%= entry.category %></div>
    </div>
    <% }); %>
    <h1>Search</h1>
  <form action="/search" method="GET">
    <label for="username">Search Name: </label>
    <input type="text" name="username" id="username">
    <button type="submit">Submit</button>
  </form>
</body>
</html>
